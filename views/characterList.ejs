<html>

<head>
    <title><%- title %></title>
</head>

<body>
    <h1><%- title %></h1>
    <ul>
        <% characters.forEach((character)=> { %>
            <li>
                <h2 style="margin: 0;"><%- character.charactername %> <a
                            href="/<%= character.player %>/<%= character.charactername %>/edit">Edit</a> <a
                            href="/<%= character.charactername %>/details">Details</a></h2>
                Server: <%- character.server %>, Level: <%- character.level %>, Job: <%- character.job %>

            </li>

            <% }) %>
    </ul>
    <a href="/">Return Home</a>
</body>

</html>